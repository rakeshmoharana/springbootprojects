package com.lambda.training;

public class Greeter {

	public void greet(Greeting greeting) {
		greeting.perform();
	
	}
	
	public static void main(String args[]) {
		Greeter greeter = new Greeter();
		HelloWorldGreeting hellowWorldGreeting = new HelloWorldGreeting();
		greeter.greet(hellowWorldGreeting );
		
		MyLambda myLambdaFunction = () -> System.out.println("Print My Lambda");
		MyAdd myLambdaAddFunction = (a,b)-> 
	}
	
	
	
	
}

interface MyAdd{
	void addInt(int a, int b);
}

interface MyLambda {
	void foo();
}